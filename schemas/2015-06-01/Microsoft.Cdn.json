        "httpPort": {
          "oneOf": [
            {
              "type": "integer"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The value of the HTTP port. Must be between 1 and 65535"
        },
        "httpsPort": {
          "oneOf": [
