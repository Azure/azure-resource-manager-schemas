  "definitions": {
    "Address": {
      "type": "object",
      "properties": {
        "address1": {
          "type": "string",
          "description": "Address 1"
        },
        "address2": {
          "type": "string",
          "description": "Address 2"
        },
        "city": {
          "type": "string",
          "description": "City"
        },
