{
  "id": "https://schema.management.azure.com/schemas/2016-01-29/Microsoft.PowerBI.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Microsoft.PowerBI",
  "description": "Microsoft Power BI Resource Types",
  "resourceDefinitions": {
    "workspaceCollections": {
      "description": "Microsoft.PowerBI/workspaceCollections resource",
      "type": "object",
      "properties": {
        "apiVersion": {
          "enum": [
            "2016-01-29"
          ]
        },
        "type": {
          "enum": [
            "Microsoft.PowerBI/workspaceCollections"
          ]
        },
        "name": {
          "oneOf": [
            {
              "type": "string",
              "pattern": "^(?:[a-zA-Z0-9]+-?)+$",
              "minLength": 3,
              "maxLength": 63
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ]
        },
        "sku": {
          "oneOf": [
            {
              "type": "object",
              "properties": {
                "name": {
                  "enum": [
                    "S1",
                    "s1"
                  ]
                },
                "tier": {
                  "enum": [
                    "Standard",
                    "standard"
                  ]
                }
              },
              "required": [
                "name"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ]
        }
      },
      "required": [
        "apiVersion",
        "type",
        "name",
        "location",
        "sku"
      ]
    }
  }
}