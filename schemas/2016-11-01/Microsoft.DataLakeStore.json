{
  "id": "http://schema.management.azure.com/schemas/2016-11-01/Microsoft.DataLakeStore.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Microsoft.DataLakeStore",
  "description": "Microsoft DataLakeStore Resource Types",
  "resourceDefinitions": {
    "accounts": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": [
            "Microsoft.DataLakeStore/accounts"
          ]
        },
        "apiVersion": {
          "type": "string",
          "enum": [
            "2016-11-01"
          ]
        },
        "location": {
          "type": "string",
          "description": "Resource location"
        },
        "tags": {
          "oneOf": [
            {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Resource tags"
        },
        "identity": {
          "oneOf": [
            {
              "$ref": "#/definitions/EncryptionIdentity"
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "The Key vault encryption identity, if any."
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/DataLakeStoreAccountProperties"
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "the Data Lake Store account properties."
        },
        "resources": {
          "type": "array",
          "items": {
            "oneOf": [
              {
                "$ref": "#/definitions/accounts_trustedIdProviders_childResource"
              },
              {
                "$ref": "#/definitions/accounts_firewallRules_childResource"
              }
            ]
          }
        }
      },
      "required": [
        "name",
        "type",
        "apiVersion",
        "location",
        "properties"
      ],
      "description": "Microsoft.DataLakeStore/accounts"
    },
    "accounts_firewallRules": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": [
            "Microsoft.DataLakeStore/accounts/firewallRules"
          ]
        },
        "apiVersion": {
          "type": "string",
          "enum": [
            "2016-11-01"
          ]
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/FirewallRuleProperties"
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "the properties of the firewall rule."
        }
      },
      "required": [
        "name",
        "type",
        "apiVersion",
        "properties"
      ],
      "description": "Microsoft.DataLakeStore/accounts/firewallRules"
    },
    "accounts_trustedIdProviders": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": [
            "Microsoft.DataLakeStore/accounts/trustedIdProviders"
          ]
        },
        "apiVersion": {
          "type": "string",
          "enum": [
            "2016-11-01"
          ]
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/TrustedIdProviderProperties"
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "the properties of the trusted identity provider."
        }
      },
      "required": [
        "name",
        "type",
        "apiVersion",
        "properties"
      ],
      "description": "Microsoft.DataLakeStore/accounts/trustedIdProviders"
    }
  },
  "definitions": {
    "accounts_firewallRules_childResource": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": [
            "firewallRules"
          ]
        },
        "apiVersion": {
          "type": "string",
          "enum": [
            "2016-11-01"
          ]
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/FirewallRuleProperties"
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "the properties of the firewall rule."
        }
      },
      "required": [
        "name",
        "type",
        "apiVersion",
        "properties"
      ],
      "description": "Microsoft.DataLakeStore/accounts/firewallRules"
    },
    "accounts_trustedIdProviders_childResource": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": [
            "trustedIdProviders"
          ]
        },
        "apiVersion": {
          "type": "string",
          "enum": [
            "2016-11-01"
          ]
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/TrustedIdProviderProperties"
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "the properties of the trusted identity provider."
        }
      },
      "required": [
        "name",
        "type",
        "apiVersion",
        "properties"
      ],
      "description": "Microsoft.DataLakeStore/accounts/trustedIdProviders"
    },
    "DataLakeStoreAccountProperties": {
      "type": "object",
      "properties": {
        "encryptionState": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Enabled",
                "Disabled"
              ]
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "The current state of encryption for this Data Lake store account."
        },
        "encryptionConfig": {
          "oneOf": [
            {
              "$ref": "#/definitions/EncryptionConfig"
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "The Key vault encryption configuration."
        },
        "firewallState": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Enabled",
                "Disabled"
              ]
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "The current state of the IP address firewall for this Data Lake store account."
        },
        "firewallRules": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/FirewallRule"
              }
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "The list of firewall rules associated with this Data Lake store account."
        },
        "trustedIdProviderState": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Enabled",
                "Disabled"
              ]
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "The current state of the trusted identity provider feature for this Data Lake store account."
        },
        "trustedIdProviders": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/TrustedIdProvider"
              }
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "The list of trusted identity providers associated with this Data Lake store account."
        },
        "defaultGroup": {
          "type": "string",
          "description": "the default owner group for all new folders and files created in the Data Lake Store account."
        },
        "newTier": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Consumption",
                "Commitment_1TB",
                "Commitment_10TB",
                "Commitment_100TB",
                "Commitment_500TB",
                "Commitment_1PB",
                "Commitment_5PB"
              ]
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "the commitment tier to use for next month."
        },
        "firewallAllowAzureIps": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Enabled",
                "Disabled"
              ]
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "The current state of allowing or disallowing IPs originating within Azure through the firewall. If the firewall is disabled, this is not enforced."
        }
      },
      "description": "Data Lake Store account properties information"
    },
    "EncryptionConfig": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "UserManaged",
            "ServiceManaged"
          ],
          "description": "The type of encryption configuration being used. Currently the only supported types are 'UserManaged' and 'ServiceManaged'."
        },
        "keyVaultMetaInfo": {
          "oneOf": [
            {
              "$ref": "#/definitions/KeyVaultMetaInfo"
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "The Key vault information for connecting to user managed encryption keys."
        }
      },
      "required": [
        "type"
      ],
      "description": "The encryption configuration for the account."
    },
    "EncryptionIdentity": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "SystemAssigned"
          ],
          "description": "The type of encryption being used. Currently the only supported type is 'SystemAssigned'."
        }
      },
      "required": [
        "type"
      ],
      "description": "The encryption identity properties."
    },
    "FirewallRule": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Resource name"
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/FirewallRuleProperties"
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "the properties of the firewall rule."
        }
      },
      "required": [
        "properties"
      ],
      "description": "Data Lake Store firewall rule information"
    },
    "FirewallRuleProperties": {
      "type": "object",
      "properties": {
        "startIpAddress": {
          "type": "string",
          "description": "the start IP address for the firewall rule. This can be either ipv4 or ipv6. Start and End should be in the same protocol."
        },
        "endIpAddress": {
          "type": "string",
          "description": "the end IP address for the firewall rule. This can be either ipv4 or ipv6. Start and End should be in the same protocol."
        }
      },
      "required": [
        "startIpAddress",
        "endIpAddress"
      ],
      "description": "Data Lake Store firewall rule properties information"
    },
    "KeyVaultMetaInfo": {
      "type": "object",
      "properties": {
        "keyVaultResourceId": {
          "type": "string",
          "description": "The resource identifier for the user managed Key Vault being used to encrypt."
        },
        "encryptionKeyName": {
          "type": "string",
          "description": "The name of the user managed encryption key."
        },
        "encryptionKeyVersion": {
          "type": "string",
          "description": "The version of the user managed encryption key."
        }
      },
      "required": [
        "keyVaultResourceId",
        "encryptionKeyName",
        "encryptionKeyVersion"
      ],
      "description": "Metadata information used by account encryption."
    },
    "TrustedIdProvider": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Resource name"
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/TrustedIdProviderProperties"
            },
            {
              "$ref": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "the properties of the trusted identity provider."
        }
      },
      "required": [
        "properties"
      ],
      "description": "Data Lake Store Trusted Identity Provider information"
    },
    "TrustedIdProviderProperties": {
      "type": "object",
      "properties": {
        "idProvider": {
          "type": "string",
          "description": "The URL of this trusted identity provider"
        }
      },
      "required": [
        "idProvider"
      ],
      "description": "Data Lake Store trusted identity provider properties information"
    }
  }
}
