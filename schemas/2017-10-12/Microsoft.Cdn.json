      },
      "required": [
        "hostName"
      ],
      "description": "The JSON object that contains the properties of the custom domain to create."
    },
    "DeepCreatedOrigin": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Origin name"
        },
        "properties": {
          "oneOf": [
