  "definitions": {
    "ConfigurationProperties": {
      "type": "object",
      "properties": {
        "source": {
          "type": "string",
          "description": "Source of the configuration."
        },
        "value": {
          "type": "string",
          "description": "Value of the configuration."
        }
      }
