        "type": {
          "type": "string",
          "enum": [
            "Microsoft.EventHub/clusters"
          ]
        },
        "apiVersion": {
          "type": "string",
          "enum": [
            "2018-01-01-preview"
          ]
        }
      },
      "required": [
        "name",
        "type",
        "apiVersion"
      ],
      "description": "Microsoft.EventHub/clusters"
    }
