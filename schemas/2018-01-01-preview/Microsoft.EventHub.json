        "type": {
          "type": "string",
          "enum": [
            "virtualnetworkrules"
          ]
        },
        "apiVersion": {
          "type": "string",
          "enum": [
            "2018-01-01-preview"
          ]
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/VirtualNetworkRuleProperties"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Properties supplied to create or update VirtualNetworkRules"
        }
      },
      "required": [
        "name",
        "type",
        "apiVersion",
        "properties"
      ],
      "description": "Microsoft.EventHub/namespaces/virtualnetworkrules"
    },
    "namespaces_networkRuleSets_childResource": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "Microsoft.EventHub/Namespaces/NetworkRuleSets"
          ]
        },
        "apiVersion": {
          "type": "string",
          "enum": [
            "2018-01-01-preview"
          ]
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/NetworkRuleSetProperties"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Properties supplied to create or update AuthorizationRule"
        }
      },
      "required": [
        "type",
        "apiVersion",
        "properties"
      ],
      "description": "Microsoft.EventHub/Namespaces/NetworkRuleSets"
    },
    "Sku": {
      "type": "object",
      "properties": {
        "name": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Basic",
                "Standard"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Name of this SKU."
        },
        "tier": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Basic",
                "Standard"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "The billing tier of this particular SKU."
        },
        "capacity": {
          "oneOf": [
            {
              "type": "integer",
              "minimum": 0,
              "maximum": 20
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "The Event Hubs throughput units, value should be 0 to 20 throughput units."
        }
      },
      "required": [
        "name"
      ],
      "description": "SKU parameters supplied to the create namespace operation"
    },
    "VirtualNetworkRuleProperties": {
      "type": "object",
      "properties": {
        "virtualNetworkSubnetId": {
          "type": "string",
          "description": "ARM ID of Virtual Network Subnet"
        }
      },
      "description": "Properties supplied to create or update VirtualNetworkRules"
    },
    "NetworkRuleSetProperties": {
      "type": "object",
      "properties": {
        "defaultAction": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Allow",
                "Deny"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Default Action for Network Rule Set"
        },
        "virtualNetworkRules": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/NWVirtualNetworkRuleProperties"
              }
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "List of VirtualNetwork Rules"
        },
        "ipRules": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/NWIpRulesProperties"
              }
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "List of IpRules"
        }
      },
      "required": [
        "defaultAction"
      ],
      "description": "Properties supplied to create or update NetworkRuleSet"
    },
    "NWIpRulesProperties": {
      "type": "object",
      "properties": {
        "ipMask": {
          "type": "string",
          "description": "IP Mask"
        },
        "action": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Allow"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
