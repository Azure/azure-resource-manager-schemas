    "EventHubEventSourceCreationProperties": {
      "type": "object",
      "properties": {
        "provisioningState": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Accepted",
                "Creating",
                "Updating",
                "Succeeded",
                "Failed",
                "Deleting"
              ]
            },
