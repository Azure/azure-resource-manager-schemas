{
  "id": "https://schema.management.azure.com/schemas/2019-01-01-preview/Microsoft.Portal.json#",
  "title": "Microsoft.Portal",
  "description": "Microsoft Portal Resource Types",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "tenant_resourceDefinitions": {
    "tenantConfigurations": {
      "type": "object",
      "properties": {
        "apiVersion": {
          "type": "string",
          "enum": [
            "2019-01-01-preview"
          ]
        },
        "name": {
          "oneOf": [
            {
              "type": "string",
              "pattern": "^.*/default$"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The resource name"
        },
        "type": {
          "type": "string",
          "enum": [
            "Microsoft.Portal/tenantConfigurations"
          ]
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/ConfigurationProperties"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Tenant configuration properties."
        }
      },
      "required": [
        "name",
        "apiVersion",
        "type",
        "properties"
      ],
      "description": "Microsoft.Portal/tenantConfigurations"
    }
  },
  "resourceDefinitions": {
    "dashboards": {
      "type": "object",
      "properties": {
        "apiVersion": {
          "type": "string",
          "enum": [
            "2019-01-01-preview"
          ]
        },
        "name": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 3,
              "maxLength": 64
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The resource name"
        },
        "type": {
          "type": "string",
          "enum": [
            "Microsoft.Portal/dashboards"
          ]
        },
        "location": {
          "type": "string",
          "description": "Resource location"
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/DashboardProperties"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The shared dashboard properties."
        },
        "tags": {
          "oneOf": [
            {
              "$ref": "#/definitions/DashboardTags"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Resource tags"
        }
      },
      "required": [
        "name",
        "apiVersion",
        "type",
        "location",
        "properties"
      ],
      "description": "Microsoft.Portal/dashboards"
    }
  },
  "definitions": {
    "ConfigurationProperties": {
      "type": "object",
      "properties": {
        "enforcePrivateMarkdownStorage": {
          "oneOf": [
            {
              "type": "boolean"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "When flag is set to true Markdown tile will require external storage configuration (URI). The inline content configuration will be prohibited."
        }
      }
    },
    "DashboardLens": {
      "type": "object",
      "properties": {
        "metadata": {
          "oneOf": [
            {
              "$ref": "#/definitions/DashboardLensMetadata"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The dashboard len's metadata."
        },
        "order": {
          "oneOf": [
            {
              "type": "integer"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The lens order."
        },
        "parts": {
          "oneOf": [
            {
              "$ref": "#/definitions/DashboardLensParts"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The dashboard parts."
        }
      },
      "required": [
        "order",
        "parts"
      ]
    },
    "DashboardLensMetadata": {
      "type": "object",
      "properties": {},
      "additionalProperties": {}
    },
    "DashboardLensParts": {
      "type": "object",
      "properties": {},
      "additionalProperties": {
        "$ref": "#/definitions/DashboardParts"
      }
    },
    "DashboardPartMetadata": {
      "type": "object",
      "properties": {},
      "additionalProperties": {}
    },
    "DashboardParts": {
      "type": "object",
      "properties": {
        "metadata": {
          "oneOf": [
            {
              "$ref": "#/definitions/DashboardPartMetadata"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "A dashboard part metadata."
        },
        "position": {
          "oneOf": [
            {
              "$ref": "#/definitions/DashboardPartsPosition"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The dashboard's part position."
        }
      },
      "required": [
        "position"
      ]
    },
    "DashboardPartsPosition": {
      "type": "object",
      "properties": {
        "colSpan": {
          "oneOf": [
            {
              "type": "integer"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The dashboard's part column span."
        },
        "metadata": {
          "oneOf": [
            {
              "$ref": "#/definitions/DashboardPartsPositionMetadata"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The dashboard part's metadata."
        },
        "rowSpan": {
          "oneOf": [
            {
              "type": "integer"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The dashboard's part row span."
        },
        "x": {
          "oneOf": [
            {
              "type": "integer"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The dashboard's part x coordinate."
        },
        "y": {
          "oneOf": [
            {
              "type": "integer"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The dashboard's part y coordinate."
        }
      },
      "required": [
        "colSpan",
        "rowSpan",
        "x",
        "y"
      ]
    },
    "DashboardPartsPositionMetadata": {
      "type": "object",
      "properties": {},
      "additionalProperties": {}
    },
    "DashboardProperties": {
      "type": "object",
      "properties": {
        "lenses": {
          "oneOf": [
            {
              "$ref": "#/definitions/DashboardPropertiesLenses"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The dashboard lenses."
        },
        "metadata": {
          "oneOf": [
            {
              "$ref": "#/definitions/DashboardPropertiesMetadata"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The dashboard metadata."
        }
      }
    },
    "DashboardPropertiesLenses": {
      "type": "object",
      "properties": {},
      "additionalProperties": {
        "$ref": "#/definitions/DashboardLens"
      }
    },
    "DashboardPropertiesMetadata": {
      "type": "object",
      "properties": {},
      "additionalProperties": {}
    },
    "DashboardTags": {
      "type": "object",
      "properties": {},
      "additionalProperties": {
        "type": "string"
      }
    }
  }
}