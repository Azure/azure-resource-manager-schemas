        },
        "azureVmFamilies": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "Unknown",
                  "Basic_A0_A4",
                  "Standard_A0_A7",
                  "Standard_A8_A11",
