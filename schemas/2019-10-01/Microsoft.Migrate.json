              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Percentile of performance data used to recommend Azure size."
        },
        "reservedInstance": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "None",
                "RI1Year",
                "RI3Year"
