        "streams": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "Microsoft-Event",
                  "Microsoft-InsightsMetrics",
                  "Microsoft-Perf",
                  "Microsoft-Syslog",
                  "Microsoft-WindowsEvent"
