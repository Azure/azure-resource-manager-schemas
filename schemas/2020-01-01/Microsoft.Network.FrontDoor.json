            {
              "enum": [
                "Enabled",
                "Disabled"
              ],
              "type": "string"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "healthProbeMethod": {
          "description": "Configures which HTTP method to use to probe the backends defined under backendPools.",
