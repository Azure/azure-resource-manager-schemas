    },
    "CacheNetworkSettings": {
      "type": "object",
      "properties": {
        "mtu": {
          "oneOf": [
            {
              "type": "integer",
              "minimum": 576,
              "maximum": 1500,
              "default": "1500"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The IPv4 maximum transmission unit configured for the subnet."
