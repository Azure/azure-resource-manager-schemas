          "description": "SKU for the Cache."
        },
        "tags": {
          "type": "object",
          "properties": {},
          "description": "ARM tags as name/value pairs."
        },
        "type": {
          "type": "string",
          "enum": [
            "Microsoft.StorageCache/caches"
          ]
        }
