      "required": [
        "targetType"
      ],
      "description": "Storage container for use as a CLFS Storage Target."
    },
    "KeyVaultKeyReference": {
      "type": "object",
      "properties": {
        "keyUrl": {
          "type": "string",
          "description": "The URL referencing a key encryption key in Key Vault."
