{
  "id": "https://schema.management.azure.com/schemas/2020-04-01/Microsoft.DataBox.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Microsoft.DataBox",
  "description": "Microsoft DataBox Resource Types",
