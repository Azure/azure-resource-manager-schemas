  "definitions": {
    "Command": {
      "type": "object",
      "properties": {
        "action": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Install",
                "Launch",
                "Close",
                "Uninstall",
                "Custom"
              ]
