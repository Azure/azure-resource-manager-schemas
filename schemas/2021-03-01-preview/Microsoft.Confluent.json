{
  "id": "https://schema.management.azure.com/schemas/2021-03-01-preview/Microsoft.Confluent.json#",
  "title": "Microsoft.Confluent",
  "description": "Microsoft Confluent Resource Types",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "subscription_resourceDefinitions": {
    "agreements": {
      "type": "object",
      "properties": {
        "apiVersion": {
          "type": "string",
          "enum": [
            "2021-03-01-preview"
          ]
        },
        "name": {
          "oneOf": [
            {
              "type": "string",
              "pattern": "^.*/default$"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The resource name"
        },
        "type": {
          "type": "string",
          "enum": [
            "Microsoft.Confluent/agreements"
          ]
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/ConfluentAgreementProperties"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Represents the properties of the resource."
        }
      },
      "required": [
        "name",
        "apiVersion",
        "type",
        "properties"
      ],
      "description": "Microsoft.Confluent/agreements"
    }
  },
  "resourceDefinitions": {
    "organizations": {
      "type": "object",
      "properties": {
        "apiVersion": {
          "type": "string",
          "enum": [
            "2021-03-01-preview"
          ]
        },
        "name": {
          "type": "string",
          "description": "The resource name"
        },
        "type": {
          "type": "string",
          "enum": [
            "Microsoft.Confluent/organizations"
          ]
        },
        "location": {
          "type": "string",
          "description": "Location of Organization resource"
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/OrganizationResourceProperties"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Organization resource properties"
        },
        "tags": {
          "oneOf": [
            {
              "$ref": "#/definitions/OrganizationResourceTags"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Organization resource tags"
        }
      },
      "required": [
        "name",
        "apiVersion",
        "type",
        "properties"
      ],
      "description": "Microsoft.Confluent/organizations"
    }
  },
  "definitions": {
    "ConfluentAgreementProperties": {
      "type": "object",
      "properties": {
        "accepted": {
          "oneOf": [
            {
              "type": "boolean"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "If any version of the terms have been accepted, otherwise false."
        },
        "licenseTextLink": {
          "type": "string",
          "description": "Link to HTML with Microsoft and Publisher terms."
        },
        "plan": {
          "type": "string",
          "description": "Plan identifier string."
        },
        "privacyPolicyLink": {
          "type": "string",
          "description": "Link to the privacy policy of the publisher."
        },
        "product": {
          "type": "string",
          "description": "Product identifier string."
        },
        "publisher": {
          "type": "string",
          "description": "Publisher identifier string."
        },
        "retrieveDatetime": {
          "type": "string",
          "description": "Date and time in UTC of when the terms were accepted. This is empty if Accepted is false."
        },
        "signature": {
          "type": "string",
          "description": "Terms signature."
        }
      }
    },
    "OfferDetail": {
      "type": "object",
      "properties": {
        "id": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 50
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Offer Id"
        },
        "planId": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 50
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Offer Plan Id"
        },
        "planName": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 50
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Offer Plan Name"
        },
        "publisherId": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 50
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Publisher Id"
        },
        "termUnit": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 25
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Offer Plan Term unit"
        }
      },
      "required": [
        "id",
        "planId",
        "planName",
        "publisherId",
        "termUnit"
      ]
    },
    "OrganizationResourceProperties": {
      "type": "object",
      "properties": {
        "offerDetail": {
          "oneOf": [
            {
              "$ref": "#/definitions/OfferDetail"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Confluent offer detail"
        },
        "userDetail": {
          "oneOf": [
            {
              "$ref": "#/definitions/UserDetail"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Subscriber detail"
        }
      },
      "required": [
        "offerDetail",
        "userDetail"
      ]
    },
    "OrganizationResourceTags": {
      "type": "object",
      "properties": {},
      "additionalProperties": {
        "type": "string"
      }
    },
    "UserDetail": {
      "type": "object",
      "properties": {
        "emailAddress": {
          "oneOf": [
            {
              "type": "string",
              "pattern": "^\\S+@\\S+\\.\\S+$"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Email address"
        },
        "firstName": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 50
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "First name"
        },
        "lastName": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 50
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Last name"
        }
      },
      "required": [
        "emailAddress"
      ]
    }
  }
}