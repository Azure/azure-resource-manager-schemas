          "type": "string",
          "description": "The location of the Template Spec Version. It must match the location of the parent Template Spec."
        },
        "name": {
          "oneOf": [
            {
              "type": "string",
              "pattern": "^[-\\w\\._\\(\\)]+$",
              "minLength": 1,
              "maxLength": 90
