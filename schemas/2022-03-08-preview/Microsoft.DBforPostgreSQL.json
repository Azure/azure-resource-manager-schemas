        "tier": {
          "description": "The tier of the particular SKU, e.g. Burstable.",
          "oneOf": [
            {
              "enum": [
                "Burstable",
                "GeneralPurpose",
                "MemoryOptimized"
              ],
              "type": "string"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
