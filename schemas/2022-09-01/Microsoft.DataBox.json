        "name": {
          "description": "The name of the job Resource within the specified resource group. job names must be between 3 and 24 characters in length and use any alphanumeric and underscore only",
          "oneOf": [
            {
              "maxLength": 24,
              "minLength": 3,
              "pattern": "^[-\\w\\.]+$",
              "type": "string"
