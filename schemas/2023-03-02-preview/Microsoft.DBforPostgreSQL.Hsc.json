            }
          ]
        },
        "startIpAddress": {
          "description": "The start IP address of the cluster firewall rule. Must be IPv4 format.",
          "oneOf": [
            {
              "pattern": "^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])$",
              "type": "string"
