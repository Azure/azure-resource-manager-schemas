  "definitions": {
    "AuthConfig": {
      "type": "object",
      "properties": {
        "activeDirectoryAuth": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "enabled",
                "disabled"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "passwordAuth": {
