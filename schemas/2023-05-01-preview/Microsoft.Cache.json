        "maxmemory-delta": {
          "type": "string",
          "description": "Value in megabytes reserved for non-cache usage per shard e.g. failover."
        },
        "maxmemory-policy": {
          "type": "string",
          "description": "The eviction strategy used when your data won't fit within its memory limit."
        },
        "maxmemory-reserved": {
          "type": "string",
          "description": "Value in megabytes reserved for non-cache usage per shard e.g. failover."
