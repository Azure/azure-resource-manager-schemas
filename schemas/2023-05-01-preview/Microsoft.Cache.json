    "RedisCacheAccessPolicyAssignmentProperties": {
      "type": "object",
      "properties": {
        "accessPolicyName": {
          "oneOf": [
            {
              "type": "string",
              "pattern": "^([a-zA-Z0-9][a-zA-Z0-9-]*[a-zA-Z0-9]|[a-zA-Z0-9])$"
