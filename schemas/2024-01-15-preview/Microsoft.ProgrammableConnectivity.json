        },
        "gatewayId": {
          "type": "string",
          "description": "Reference to the APC Gateway resource ID."
        },
        "operatorApiPlanId": {
          "type": "string",
          "description": "Reference to the Operator API Plan Resource ID."
        },
        "saasProperties": {
          "oneOf": [
