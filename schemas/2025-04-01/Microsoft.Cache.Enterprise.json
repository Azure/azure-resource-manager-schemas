                "MemoryOptimized_M50",
                "MemoryOptimized_M100",
                "MemoryOptimized_M150",
                "MemoryOptimized_M250",
                "MemoryOptimized_M350",
                "MemoryOptimized_M500",
                "MemoryOptimized_M700",
                "MemoryOptimized_M1000",
                "MemoryOptimized_M1500",
                "MemoryOptimized_M2000",
                "ComputeOptimized_X3",
                "ComputeOptimized_X5",
