          "enum": [
            "Microsoft.Cache/redisEnterprise"
          ],
          "type": "string"
        },
        "zones": {
          "description": "The Availability Zones where this cluster will be deployed.",
          "oneOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        }
      },
      "required": [
        "name",
        "properties",
        "apiVersion",
        "type"
      ],
