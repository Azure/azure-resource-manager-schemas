{
  "$schema": "https://raw.githubusercontent.com/Azure/azure-rest-api-specs/master/documentation/sdkautomation/SwaggerToSdkConfigSchema.json",
  "initOptions": {
    "initScript": {
      "path": "npm install --prefix generator"
    }
  },
  "generateOptions": {
    "preprocessDryRunGetPackageName": false,
    "parseGenerateOutput": false,
    "generateScript": {
      "path": "npm run generate-single"
    }
  },
  "advancedOptions": {
    "closeIntegrationPR": false,
    "draftIntegrationPR": false,
    "generationCallMode": "one-per-config"
  }
}
