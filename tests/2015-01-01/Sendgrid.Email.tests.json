{
  "tests": [
    {
      "name": "SendGrid - valid params",
      "definition": "https://schema.management.azure.com/schemas/2015-01-01/Sendgrid.Email.json#/resourceDefinitions/accounts",
      "json": {
        "type": "Sendgrid.Email/accounts",
        "name": "my name",
        "plan": {
          "name": "Free",
          "publisher": "Sendgrid",
          "product": "sendgrid_azure"
        },
        "apiVersion": "2015-01-01",
        "properties": {
          "password": "mypassword",
          "acceptMarketingEmails": true,
          "email": "myemail@domain.com"
        }
      }
    },
    {
      "name": "SendGrid - errors first level",
      "definition": "https://schema.management.azure.com/schemas/2015-01-01/Sendgrid.Email.json#/resourceDefinitions/accounts",
      "expectedErrors": [
        {
          "message": "Missing required property: name",
          "dataPath": "/"
        },
        {
          "message": "Missing required property: type",
          "dataPath": "/"
        },
        {
          "message": "Missing required property: plan",
          "dataPath": "/"
        },
        {
          "message": "Missing required property: apiVersion",
          "dataPath": "/"
        },
        {
          "message": "Missing required property: properties",
          "dataPath": "/"
        }
      ],
      "json": {}
    },
    {
      "name": "SendGrid - errors second level",
      "definition": "https://schema.management.azure.com/schemas/2015-01-01/Sendgrid.Email.json#/resourceDefinitions/accounts",
      "expectedErrors": [
        {
          "message": "Missing required property: name",
          "dataPath": "/plan"
        },
        {
          "message": "Missing required property: publisher",
          "dataPath": "/plan"
        },
        {
          "message": "Missing required property: product",
          "dataPath": "/plan"
        },
        {
          "message": "Missing required property: password",
          "dataPath": "/properties"
        },
        {
          "message": "Missing required property: acceptMarketingEmails",
          "dataPath": "/properties"
        },
        {
          "message": "Missing required property: email",
          "dataPath": "/properties"
        }
      ],
      "json": {
        "type": "Sendgrid.Email/accounts",
        "name": "my name",
        "plan": {},
        "apiVersion": "2015-01-01",
        "properties": {}
      }
    },
    {
      "name": "SendGrid - invalid apiVersion",
      "definition": "https://schema.management.azure.com/schemas/2015-01-01/Sendgrid.Email.json#/resourceDefinitions/accounts",
      "expectedErrors": [
        {
          "message": "No enum match for: \"2014-01-01\"",
          "dataPath": "/apiVersion"
        }
      ],
      "json": {
        "type": "Sendgrid.Email/accounts",
        "name": "my name",
        "plan": {
          "name": "Free",
          "publisher": "Sendgrid",
          "product": "sendgrid_azure"
        },
        "apiVersion": "2014-01-01",
        "properties": {
          "password": "mypassword",
          "acceptMarketingEmails": true,
          "email": "myemail@domain.com"
        }
      }
    },
    {
      "name": "SendGrid - invalid type",
      "definition": "https://schema.management.azure.com/schemas/2015-01-01/Sendgrid.Email.json#/resourceDefinitions/accounts",
      "expectedErrors": [
        {
          "message": "No enum match for: \"Sendgrid.Type.Invalid\"",
          "dataPath": "/type"
        }
      ],
      "json": {
        "type": "Sendgrid.Type.Invalid",
        "name": "my name",
        "plan": {
          "name": "Free",
          "publisher": "Sendgrid",
          "product": "sendgrid_azure"
        },
        "apiVersion": "2015-01-01",
        "properties": {
          "password": "mypassword",
          "acceptMarketingEmails": true,
          "email": "myemail@domain.com"
        }
      }
    },
    {
      "name": "SendGrid - invalid plan.name",
      "definition": "https://schema.management.azure.com/schemas/2015-01-01/Sendgrid.Email.json#/resourceDefinitions/accounts",
      "expectedErrors": [
        {
          "message": "Data does not match any schemas from \"oneOf\"",
          "dataPath": "/plan/name"
        }
      ],
      "json": {
        "type": "Sendgrid.Email/accounts",
        "name": "my name",
        "plan": {
          "name": "PlanInvalid",
          "publisher": "Sendgrid",
          "product": "sendgrid_azure"
        },
        "apiVersion": "2015-01-01",
        "properties": {
          "password": "mypassword",
          "acceptMarketingEmails": true,
          "email": "myemail@domain.com"
        }
      }
    },
    {
      "name": "SendGrid - invalid plan.product",
      "definition": "https://schema.management.azure.com/schemas/2015-01-01/Sendgrid.Email.json#/resourceDefinitions/accounts",
      "expectedErrors": [
        {
          "message": "Data does not match any schemas from \"oneOf\"",
          "dataPath": "/plan/product"
        }
      ],
      "json": {
        "type": "Sendgrid.Email/accounts",
        "name": "my name",
        "plan": {
          "name": "Free",
          "publisher": "Sendgrid",
          "product": "free_id_invalid"
        },
        "apiVersion": "2015-01-01",
        "properties": {
          "password": "mypassword",
          "acceptMarketingEmails": true,
          "email": "myemail@domain.com"
        }
      }
    },
    {
      "name": "SendGrid - min length properties.password",
      "definition": "https://schema.management.azure.com/schemas/2015-01-01/Sendgrid.Email.json#/resourceDefinitions/accounts",
      "expectedErrors": [
        {
          "message": "Data does not match any schemas from \"anyOf\"",
          "dataPath": "/properties/password"
        }
      ],
      "json": {
        "type": "Sendgrid.Email/accounts",
        "name": "my name",
        "plan": {
          "name": "Free",
          "publisher": "Sendgrid",
          "product": "sendgrid_azure"
        },
        "apiVersion": "2015-01-01",
        "properties": {
          "password": "abc",
          "acceptMarketingEmails": true,
          "email": "myemail@domain.com"
        }
      }
    },
    {
      "name": "SendGrid - max length properties.password",
      "definition": "https://schema.management.azure.com/schemas/2015-01-01/Sendgrid.Email.json#/resourceDefinitions/accounts",
      "expectedErrors": [
        {
          "message": "Data does not match any schemas from \"anyOf\"",
          "dataPath": "/properties/password"
        }
      ],
      "json": {
        "type": "Sendgrid.Email/accounts",
        "name": "my name",
        "plan": {
          "name": "Free",
          "publisher": "Sendgrid",
          "product": "sendgrid_azure"
        },
        "apiVersion": "2015-01-01",
        "properties": {
          "password": "longpasswordlongpasswordlongpasswordlongpasswordlon",
          "acceptMarketingEmails": true,
          "email": "myemail@domain.com"
        }
      }
    },
    {
      "name": "SendGrid - min length name",
      "definition": "https://schema.management.azure.com/schemas/2015-01-01/Sendgrid.Email.json#/resourceDefinitions/accounts",
      "expectedErrors": [
        {
          "message": "Data does not match any schemas from \"anyOf\"",
          "dataPath": "/name"
        }
      ],
      "json": {
        "type": "Sendgrid.Email/accounts",
        "name": "abc",
        "plan": {
          "name": "Free",
          "publisher": "Sendgrid",
          "product": "sendgrid_azure"
        },
        "apiVersion": "2015-01-01",
        "properties": {
          "password": "mypassword",
          "acceptMarketingEmails": true,
          "email": "myemail@domain.com"
        }
      }
    },
    {
      "name": "SendGrid - max length name",
      "definition": "https://schema.management.azure.com/schemas/2015-01-01/Sendgrid.Email.json#/resourceDefinitions/accounts",
      "expectedErrors": [
        {
          "message": "Data does not match any schemas from \"anyOf\"",
          "dataPath": "/name"
        }
      ],
      "json": {
        "type": "Sendgrid.Email/accounts",
        "name": "longnamelongnamelongnamelongnamelongnamelongnamelon",
        "plan": {
          "name": "Free",
          "publisher": "Sendgrid",
          "product": "sendgrid_azure"
        },
        "apiVersion": "2015-01-01",
        "properties": {
          "password": "mypassword",
          "acceptMarketingEmails": true,
          "email": "myemail@domain.com"
        }
      }
    }
  ]
}