{
  "tests": [
    {
      "name": "ApiManagement Service Apis - Minimal",
      "definition": "https://schema.management.azure.com/schemas/2017-03-01/Microsoft.ApiManagement.json#/resourceDefinitions/service_apis",
      "json": {
        "apiVersion": "2017-03-01",
        "type": "Microsoft.ApiManagement/service/apis",
        "name": "57d1f7558aa04f15146d9d8a",
        "properties": {
          "displayName": "Service",
          "apiRevision": "1",
          "serviceUrl": "https://api.plexonline.com/DataSource/Service.asmx",
          "path": "schulte",
          "protocols": [
            "https"
          ],
          "subscriptionKeyParameterNames": {
            "header": "Ocp-Apim-Subscription-Key",
            "query": "subscription-key"
          },
          "type": "soap",
          "isCurrent": true,
          "isOnline": true
        }
      }
    },
    {
      "name": "ApiManagement service - invalidType",
      "expectedErrors": [
        {
          "message": "No enum match for: \"Microsoft.ApiManagement/invalidType\"",
          "dataPath": "/type",
          "schemaPath": "/properties/type/type"
        },
        {
          "message": "No enum match for: \"2015-10-12\"",
          "dataPath": "/apiVersion",
          "schemaPath": "/properties/apiVersion/type"
        }
      ],
      "definition": "https://schema.management.azure.com/schemas/2017-03-01/Microsoft.ApiManagement.json#/resourceDefinitions/service_apis",
      "json": {
        "type": "Microsoft.ApiManagement/invalidType",
        "apiVersion": "2015-10-12",
        "name": "57d1f7558aa04f15146d9d8a",
        "properties": {
          "displayName": "Service",
          "apiRevision": "1",
          "serviceUrl": "https://api.plexonline.com/DataSource/Service.asmx",
          "path": "schulte",
          "protocols": [
            "https"
          ],
          "subscriptionKeyParameterNames": {
            "header": "Ocp-Apim-Subscription-Key",
            "query": "subscription-key"
          },
          "type": "soap",
          "isCurrent": true,
          "isOnline": true
        }
      }
    }
  ]
}