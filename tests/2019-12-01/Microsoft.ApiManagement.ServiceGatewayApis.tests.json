{
  "tests": [
    {
      "name": "ApiManagement Service Gateway Api Operation - Minimal",
      "definition": "https://schema.management.azure.com/schemas/2019-12-01/Microsoft.ApiManagement.json#/resourceDefinitions/service_gateways_apis",
      "json": {
        "apiVersion": "2019-12-01",
        "type": "Microsoft.ApiManagement/service/gateways/apis",
        "name": "echoapi",
        "properties": {
          "displayName": "EchoApi",
          "apiRevision": "1",
          "serviceUrl": "https://contoso.com/apis/echo",
          "path": "",
          "protocols": [
            "http",
            "https"
          ],
          "subscriptionKeyParameterNames": {
            "header": "Ocp-Apim-Subscription-Key",
            "query": "subscription-key"
          },
          "isCurrent": true
        }
      }
    },
    {
      "name": "ApiManagement Service Gateway Api Operation - invalidType",
      "expectedErrors": [
        {
          "message": "No enum match for: \"Microsoft.ApiManagement/invalidType\"",
          "dataPath": "/type",
          "schemaPath": "/properties/type/type"
        },
        {
          "message": "No enum match for: \"2015-10-12\"",
          "dataPath": "/apiVersion",
          "schemaPath": "/properties/apiVersion/type"
        }
      ],
      "definition": "https://schema.management.azure.com/schemas/2019-12-01/Microsoft.ApiManagement.json#/resourceDefinitions/service_gateways_apis",
      "json": {
        "type": "Microsoft.ApiManagement/invalidType",
        "apiVersion": "2015-10-12",
        "name": "echoapi",
        "properties": {
          "displayName": "EchoApi",
          "apiRevision": "1",
          "serviceUrl": "https://contoso.com/apis/echo",
          "path": "",
          "protocols": [
            "http",
            "https"
          ],
          "subscriptionKeyParameterNames": {
            "header": "Ocp-Apim-Subscription-Key",
            "query": "subscription-key"
          },
          "isCurrent": true
        }
      }
    }
  ]
}