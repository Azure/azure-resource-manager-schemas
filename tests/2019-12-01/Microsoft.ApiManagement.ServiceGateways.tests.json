{
  "tests": [
    {
      "name": "ApiManagement Service Gateway Operation - Minimal",
      "definition": "https://schema.management.azure.com/schemas/2019-12-01/Microsoft.ApiManagement.json#/resourceDefinitions/service_gateways",
      "json": {
        "apiVersion": "2019-12-01",
        "type": "Microsoft.ApiManagement/service/gateways",
        "name": "gateway1",
        "properties": {
          "description": "my gateway 1",
          "locationData": {
            "name": "my location"
          }
        }
      }
    },
    {
      "name": "ApiManagement Service Gateway Operation - invalidType",
      "expectedErrors": [
        {
          "message": "No enum match for: \"Microsoft.ApiManagement/invalidType\"",
          "dataPath": "/type",
          "schemaPath": "/properties/type/type"
        },
        {
          "message": "No enum match for: \"2015-10-12\"",
          "dataPath": "/apiVersion",
          "schemaPath": "/properties/apiVersion/type"
        }
      ],
      "definition": "https://schema.management.azure.com/schemas/2019-12-01/Microsoft.ApiManagement.json#/resourceDefinitions/service_gateways",
      "json": {
        "type": "Microsoft.ApiManagement/invalidType",
        "apiVersion": "2015-10-12",
        "name": "gateway1",
        "properties": {
          "description": "my gateway 1",
          "locationData": {
            "name": "my location"
          }
        }
      }
    }
  ]
}