{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "namespaces_testscriptsb_name": {
      "type": "String"
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.ServiceBus/namespaces",
      "apiVersion": "2021-11-01",
      "name": "[parameters('namespaces_testscriptsb_name')]",
      "location": "West US",
      "sku": {
        "name": "Standard",
        "tier": "Standard"
      },
      "properties": {
        "disableLocalAuth": false,
        "zoneRedundant": false
      }
    }
  ]
}