{
  "tests": [
    {
      "name": "DataMigrationServices",
      "definition": "https://schema.management.azure.com/schemas/2017-11-15-preview/Microsoft.DataMigration.json#/resourceDefinitions/services",
      "json": {      
        "type": "Microsoft.DataMigration/services",
        "apiVersion": "2017-11-15-preview",
        "name": "[parameters('Name')]",
        "location": "[parameters('Location')]",
        "sku": {
            "name": "[parameters('Sku')]"
        },
        "properties": {
            "virtualSubnetId": 
                "/subscriptions/fc04246f-04c5-437e-ac5e-206a19e7193f/resourceGroups/AdamTest/providers/Microsoft.Network/virtualNetworks/AdamTestSCUS/subnets/default"
        }
      }        
    },
    {
      "name": "DataMigrationProjects",
      "definition": "https://schema.management.azure.com/schemas/2017-11-15-preview/Microsoft.DataMigration.json#/resourceDefinitions/services_projects",
      "json": {      
        "type": "Microsoft.DataMigration/services/projects",
        "apiVersion": "2017-11-15-preview",
        "name": "[parameters('Name')]",
        "location": "[parameters('Location')]",
        "properties": {
            "sourcePlatform": "SQL",
            "targetPlatform": "SQLDB"
        }
      }        
    }
  ]
}