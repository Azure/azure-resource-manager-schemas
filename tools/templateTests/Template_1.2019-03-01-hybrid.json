{
  "$schema": "https://schema.management.azure.com/schemas/2019-03-01-hybrid/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "keyvaultSettings": {
      "type": "string"
    },
    "keyvaultSecrets": {
      "type": "string"
    },
    "keyvaultAP":{
      "type":"string"
    }
  },
  "variables": {
    "loadBalancerName": {
      "type": "string",
      "metadata": {
        "description": "This is the name of the load balancer"
      }
    },
    "publicIPAddressID1": "123"
  },
  "outputs": {
    "out1": {
      "type": "secureObject",
