            "tenantId": "[parameters('keyvaultSecrets').tenantId]",
            "objectId": "[parameters('keyvaultSecrets').objectId]",
            "permissions": {
              "keys": [
                "get"
              ],
              "secrets": [
                "get"
              ]
            }
          }
        ],
        "sku": {
          "name": "[parameters('keyvaultSecrets').vaultSku]",
